{
  "name": "@servicerocket/scaff-cloud-frontend",
  "author": "ServiceRocket",
  "license": "UNLICENSED",
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "build": "yarn lint && cross-env WEBPACK_MODE=production webpack",
    "clean": "del-cli build/",
    "deploy": "s3-deploy './build/**' --cwd './build/' --deleteRemoved",
    "deploy:dev": "yarn build && yarn deploy",
    "lint": "tslint --project ./tsconfig.json --config ../tslint.json",
    "local": "webpack serve",
    "pre-commit": "lint-staged",
    "test": "yarn run lint && jest --silent --verbose",
    "test:coverage": "jest --coverage --silent --verbose",
    "test:updateSnapshot": "jest --updateSnapshot --silent --verbose"
  },
  "jestSonar": {
    "reportPath": "coverage",
    "reportFile": "test-reporter.xml",
    "indent": 4
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "yarn lint --fix",
      "prettier --ignore-path ../.prettierignore --config ../.prettierrc.json --write ."
    ]
  },
  "dependencies": {
    "@atlaskit/analytics-next": "^8.1.1",
    "@atlaskit/button": "^15.1.4",
    "@atlaskit/checkbox": "^12.0.0",
    "@atlaskit/datetime-picker": "^10.0.7",
    "@atlaskit/editor-common": "^55.2.1",
    "@atlaskit/editor-core": "^139.0.0",
    "@atlaskit/form": "^8.2.2",
    "@atlaskit/icon": "^21.1.4",
    "@atlaskit/icon-object": "^6.2.0",
    "@atlaskit/inline-dialog": "^13.0.9",
    "@atlaskit/media-core": "^32.1.2",
    "@atlaskit/renderer": "^73.0.0",
    "@atlaskit/select": "^13.1.1",
    "@atlaskit/smart-card": "^14.6.1",
    "@atlaskit/spinner": "^15.0.6",
    "@atlaskit/textarea": "^4.0.2",
    "@atlaskit/textfield": "^5.0.0",
    "@atlaskit/theme": "^11.0.2",
    "@atlaskit/tooltip": "^17.1.2",
    "@atlaskit/user-picker": "^7.2.1",
    "@servicerocket/scaff-cloud-field-group-utils": "^0.0.1",
    "@types/dompurify": "^2.2.1",
    "@ungap/url-search-params": "^0.2.2",
    "assert": "^2.0.0",
    "buffer": "^6.0.3",
    "copy-webpack-plugin": "^7.0.0",
    "dompurify": "^2.2.6",
    "lodash.clonedeep": "^4.5.0",
    "lodash.findindex": "^4.6.0",
    "moment": "^2.29.1",
    "process": "^0.11.10",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.8.0",
    "react-intl": "^2.9.0",
    "remark": "^13.0.0",
    "rxjs": "^5.5.0",
    "stream-browserify": "^3.0.0",
    "strip-markdown": "^4.0.0",
    "styled-components": "^3.4.10",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@testing-library/dom": "^7.16.2",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@testing-library/user-event": "^12.8.1",
    "cross-env": "^7.0.2",
    "enzyme": "^3.11.0",
    "enzyme-to-json": "^3.5.0",
    "react-test-renderer": "^17.0.1",
    "s3-deploy": "^1.4.0",
    "source-map-loader": "^2.0.0",
    "tslint-react-hooks": "^2.2.2",
    "webpack-dev-server": "^3.11.0"
  }
}

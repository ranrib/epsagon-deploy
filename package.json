{
  "name": "epsagon-deploy",
  "version": "1.0.0",
  "description": "Serverless webpack example using Typescript",
  "main": "handler.js",
  "private": true,
  "workspaces": [
    "app",
    "field-group-utils",
    "frontend"
  ],
  "scripts": {
    "add:app": "lerna add --scope=@servicerocket/scaff-cloud-app",
    "add:frontend": "lerna add --scope=@servicerocket/scaff-cloud-frontend",
    "build": "lerna run --stream build",
    "build:clean": "yarn clean && yarn && yarn build",
    "build:app": "lerna run --stream --scope @servicerocket/scaff-cloud-app --include-dependencies build",
    "build:frontend": "lerna run --stream --scope @servicerocket/scaff-cloud-frontend --include-dependencies build",
    "build:dependencies": "lerna run --stream --scope @servicerocket/scaff-cloud-field-group-utils build",
    "clean": "yarn clean:packages && yarn clean:root",
    "clean:packages": "lerna run --stream clean && lerna clean",
    "clean:root": "del-cli node_modules",
    "deploy:dev": "yarn build:dependencies && lerna run --stream deploy:dev --",
    "deploy:app": "lerna run --stream --scope @servicerocket/scaff-cloud-app deploy --",
    "deploy:app:dev": "yarn build:dependencies && lerna run --stream --scope @servicerocket/scaff-cloud-app deploy:dev --",
    "deploy:frontend": "lerna run --stream --scope @servicerocket/scaff-cloud-frontend deploy --",
    "deploy:frontend:dev": "yarn build:dependencies && lerna run --stream --scope @servicerocket/scaff-cloud-frontend deploy:dev --",
    "exec:app": "lerna exec --scope @servicerocket/scaff-cloud-app --",
    "exec:frontend": "lerna exec --scope @servicerocket/scaff-cloud-frontend --",
    "lint": "lerna run --stream lint --",
    "lint:watch": "lerna run --stream lint:watch",
    "logs:app": "lerna run --stream --scope @servicerocket/scaff-cloud-app logs --",
    "ls": "lerna ls",
    "postinstall": "lerna bootstrap",
    "register:dev": "lerna run --stream --scope @servicerocket/scaff-cloud-app register:dev --",
    "sonar": "sonar-scanner",
    "test": "lerna run --stream test --",
    "test:app": "lerna run --stream --scope @servicerocket/scaff-cloud-app test --",
    "test:coverage": "lerna run --stream test:coverage --",
    "test:frontend": "lerna run --stream --scope @servicerocket/scaff-cloud-frontend test --",
    "test:dependencies": "lerna run --stream --scope @servicerocket/scaff-cloud-field-group-utils test --"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lerna run --concurrency 1 --stream pre-commit"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@types/aws-lambda": "^8.10.66",
    "@types/chance": "^1.1.0",
    "@types/jest": "^26.0.20",
    "@types/lodash.clonedeep": "^4.5.6",
    "@types/node": "^14.14.31",
    "@types/react": "^16.9.56",
    "@types/react-dom": "^16.8.0",
    "@types/react-intl": "^3.0.0",
    "@types/react-select": "^3.0.26",
    "@types/react-test-renderer": "^17.0.0",
    "@types/testing-library__react": "^10.0.1",
    "babel-jest": "^26.0.1",
    "chance": "^1.1.7",
    "child_process": "^1.0.2",
    "fork-ts-checker-webpack-plugin": "^3.1.1",
    "husky": "^4.3.7",
    "jest": "^26.6.1",
    "jest-sonar-reporter": "^2.0.0",
    "jest-styled-components": "^6.3.4",
    "lerna": "^3.22.1",
    "lint-staged": "^10.5.3",
    "prettier": "2.2.1",
    "sonarqube-scanner": "^2.8.0",
    "ts-jest": "^26.4.1",
    "ts-loader": "^8.0.18",
    "ts-node": "^9.1.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "tslint-xo": "^0.11.0",
    "typescript": "^4.0.3",
    "webpack": "^5.5.1",
    "webpack-cli": "^4.5.0"
  },
  "resolutions": {
    "prosemirror-model": "1.11.0",
    "@types/react": "^16.9.56"
  },
  "author": "The serverless webpack authors (https://github.com/elastic-coders/serverless-webpack)",
  "license": "MIT"
}
